/* base styles 
========================================================*/

/*changes selected type color*/
::selection {
	background: #e8ad2d; /* Safari */
	color: #fff;	
}
::-moz-selection {
	background: #e8ad2d; /* Firefox */
	color: #fff;
}

* { -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }


body {
       font-family: 'ColaborateLightRegular', helvetica, arial, sans-serif;
       -webkit-font-smoothing: antialiased;
       //background-image: url("../images/brillant/brillant.png");
       background-image: url("../images/subtle_dots/subtle_dots.png");
       color: $mainColor;
       //text-shadow: 0px 1px 2px #fff;
       background-color: #fff;
       line-height: 1.3em;
       font-size: 1.3em;
}

.container {
/* 	width: 90%; */
	width: $maxWidth;
	padding-top: 30px;
	margin: 0 auto;
}

img {
	max-width: 100%;
	height: auto;	
}


a {
	color: $link;
	text-decoration: none;
	&:hover {color: darken($link,15);}
}

p {
	font-size: 1.3em;
	line-height: 1.3em;
}

.info {
	color: #cbcbc2;
	font-size: .7em;
	a {color: #cbcbc2;}
}

article {
/* 	width: 70%; */
	width: $widthMain;
	float: left;
	footer {color: #999; font-size: .7em;}
	margin-bottom: 2em;
}

/*
.articleClear {
	margin-top: 15em;
}
*/

article {
	h2 { 
		font-size: 1.8em; 
		margin-top: .1em; 
		margin-bottom: .1em; 
		line-height: 1.1em; 
		font-family: bree-serif, helvetica, arial, sans-serif;
		}
	h1 {font-size: 1.8em;}
}

footer {
	clear: both;
	text-align: left;
	font-size: .6em;
	color: lighten($mainColor,20);
	padding-top:2em;
	padding-bottom:2em;
}

.mainFooter {
/* 	background-color: $link; */
	width: $maxWidth;
	margin: 0 auto;
}

article footer {
	text-align: left;
	padding: 0px;
	a {color: #333;}
}

.hilight {
	font-size: 1.3em;
	color: $link;
}

.nextprev {
	float: left;
	clear: both;
}

.captions {
	font-size: .6em;
	color: lighten($mainColor,40);
}

/* header 
=============================================================*/

header ul li { 
	display: block;
	margin-right: .6em;
	padding: .3em;	
	float: left;
	@extend .trans;
	a {
		color: #e8ad2d; 
		&:hover {
			padding-left: 0;
			color: #bc8d27;
		}    
	}
}

.mainnav {
	width: 100%;
/* 	height: 65px; */
}

header {
	width: 100%;
	margin-bottom: 20px;
}

header h1 {
	background-repeat: no-repeat;
	color: $link; font-family: bree-serif, helvetica, arial, sans-serif;
}

.current_page_item {
	a {
	color: $link2;
	font-weight: bold;
	border-bottom: 3px solid $link;	
	}
}

/* for logo image replacement */
.hide-text {
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}

.logoimg{
	height: 75px;
	width: 75px;
	background-image: url("../images/tylogoyel.svg"); 
/* 	background-image: url("../images/logo_yellow75.jpg");  */
	background-repeat: no-repeat;
	display: block;
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}

.logo_margin {
	margin: 0px 50px 0px 0px;
/* 	padding-left: 30px; */
}

.logo {
	float: left;
/* 	width: 250px; */
}

nav {
	line-height: 50px;
}



/* sidebar 
=============================================================*/

aside {
	width: $widthSide;
	float: right;
	padding-top:1em;
}

aside li {
	list-style: none; 
}

aside #s {
	width: $widthSide;
}

.c {
	position: relative;
	padding-top: 1em;
}


.sidebar{
	font-size: 1em;
	position: absolute;
	top: 0px;
	right: 0px;
	}
	h3 {color: #e8ad2d; font-size: 1.2em; margin: 0px; padding-bottom: 10px; font-family: bree-serif, helvetica, arial, sans-serif;}
	li a{
		@extend .trans;
		border-bottom: 1px solid $link;
	color: $mainColor; 
	&:hover {
		color: $link;
		padding-left: 2px;
	} 
	line-height: 1.1em;
	display: block;
	clear: both;
	padding-top: 8px;
	padding-bottom: 8px;
	//border-bottom: 1px solid $link;
	.textwidget {color: #928465; font-size: .8em; line-height: 1.2em;}
	}

aside ul {
	list-style: none;
	padding: 0px;
}



/* About template *****************************************/

.aboutMain {
	width: $widthMain;
	float: left;
	clear: both;
	padding: 0;
	p {
		margin: .5em;
		padding: 0;
	}
}

.rounded {
	background-color: $link;
	padding: 5px;
	text-align: center;
	box-shadow: inset 0px 0px 10px rgba(0,0,0,.2);
	@include border-radius($corner);
	color: #fff;
	font-size: 1em;
	
}

.sidebarabout {
	font-family: bree-serif, serif;
	//margin-top: 0;
	p { font-size: .8em; color: $link;}
		padding: 0 0 0 0;

}

/* TOGGLE NAV    =================================== */

		a.menu-link {
			color: #fff;
			font-family: bree-serif;
  	    	float: right;
		    display: block;
		    margin-top: .5em;
		    background-color: $link;
		    text-shadow: 0px 1px 2px $linkshadow;
			padding: .5em;
			text-align: center;
		    @include border-radius($corner);
		    padding-top: .6em;
		}
		nav[role=navigation] {
			-webkit-transition: all ease-out;  
			-moz-transition: all 0.3s ease-out;
			-ms-transition: all 0.3s ease-out;
			-o-transition: all 0.3s ease-out;
			transition: all ease-out;
		}
		.js nav[role=navigation] {
			overflow: hidden;
			max-height: 0;
		}
		nav[role=navigation].active {
			max-height: 15em;
			padding-top: 20px;
			width: 100%;
			li {
				width: 100%;
				background-color: $link;
				@include border-radius($corner);
				:hover { background-color: #b57d03;}
				a { color: #fff;
					font-family: bree-serif;
					font-size: 1em;
					text-shadow: 0px 1px 2px $linkshadow;
				 }
			}
		}
	
		
	
		nav[role=navigation] ul {
			margin: 0;
			padding: 0;
/* 			border-top: 1px solid $link; */
			li {
				margin-top: .2em;
				margin-bottom: .2em;
			}
		}
		nav[role=navigation] li a {
			display: block;
/* 			padding: 0.1em; */
			text-align: center;
/* 			border-bottom: 1px solid $link; */
		}
		
		@media screen and (min-width: 35em) {
			a.menu-link {
			   display: none;
			}
			.js nav[role=navigation] {
				max-height: none;
			}
			nav#menu [role=navigation] {
				clear: both;
			}
			nav[role=navigation] ul {
				margin: 0 0 0 -0.25em;
				border: 0;
			}
			
			nav[role=navigation]  li {
				display: inline-block;
				margin: 0 0.25em;
			}
			nav[role=navigation] li a {
/* 				border: 0; */
			}
		}

/*Work template *****************************************/

.workMain {
	width: 100%;
	float: left;
	clear: both;
}


/*Work ****************************************************/

		.g {
/* 			background-color: green; */
  			padding: 0.25em;
  			overflow: hidden;
		}
		.g li {
/* 			clear: both; */
			float: left;
			width: 100%;
			padding: 0.25em;
			list-style: none;
		}
		.g img {
			display: block;
		}
		.g li:nth-child(odd) {
			clear: left;
		}
		@media screen and (min-width: 30em) {
			.g {
/* 				background-color: black; */
			}
			.g li {
		    	width: 45%; 
		  	}
		  	.g li:nth-child(2n+1) {
		  		clear: left;
		  	}
		  	.g li:nth-child(odd) {
				clear: none;
			}
		}

		
		@media screen and (min-width: 40em) {
			.g {
/* 				background-color: grey; */
			}
			.g li {
		    	width: 31%; 
		  	}
		  	.g li:nth-child(3n+1) {
		  		clear: left;
		  	}
		  	.g li:nth-child(odd) {
				clear: none;
			}
		}
		@media screen and (min-width: 60em) {
			.g {
/* 				background-color: fuchsia; */
			}
			.g li {
		    	width: 23%; 
		  	}
		  	.g li:nth-child(4n+1) {
		  		clear: left;
		  	}
		  	.g li:nth-child(3n+1) {
				clear: none;
			}
		}
		@media screen and (min-width: 72em) {
			.g { 
/* 				background-color: yellow; */
			}
			.g li {
		    	width: 18%; 
		  	}
		  	.g li:nth-child(5n+1) {
		  		clear: left;
		  	}
		  	.g li:nth-child(4n+1) {
				clear: none;
			}
		}
		@media screen and (min-width: 90em) {
			.g {
/* 				background-color: aqua; */
			}
			.g li {
		    	width: 15%; 
		  	}
		  	.g li:nth-child(6n+1) {
		  		clear: left;
		  	}
		  	.g li:nth-child(5n+1) {
				clear: none;
			}
		}
		
		
/* COMMENTS **********************/
#respond {
	clear: both;
}

h3#reply-title {
	color: #e8ad2d; 
	font-size: 1.2em; 
	margin: 0px; 
	padding-bottom: 10px; 
	font-family: bree-serif, helvetica, arial, sans-serif;
}
p.logged-in-as {
	font-size: 1em;
	margin: 0;
}
textarea#comment {
	color: $mainColor;
	//background-color: #fff;
	font-family: 'ColaborateLightRegular', helvetica, arial, sans-serif;
	border: 1px solid $link;
	padding: .6em;
	display: block;
	clear: both;
}
input#submit {
	@include border-radius($corner);
	padding: .5em;
	background-color: $link;
	color: #fff;
	border: 0;
	&:hover { background-color: $link2;}
}
#comments-title {
	clear: both;
}
.comment-form-comment label {
	clear: both;
}
.form-allowed-tags {
	display: none;
}


/* NetBook **********************/


@media (max-width: 70em) {
  


.sidebar, .sidebarabout {
	clear: both;
	width: 100%;
	margin-top: 2em;
	position: relative;
}

.container {
	width: $maxWidth;
	padding-top: 30px;
	margin: 0px auto;
}

p {
	font-size: 1.3em;
}

article {
	width: 100%;
	margin: 0 auto;
}

nav {
/* 	clear: both; */
	padding-left: 0px;
/* 	margin: 0 auto; */
/* 	float: left; */
	ul {
		padding-left: 0px;
	}
}

header ul li { 
	display: block;
	margin-right: .2em;
	a {font-size: 1em;}
}


article {
	h2 { font-size: 1.6em; margin-top: .5em; line-height: 1.1em;}
/* 	p { font-size: 2em; line-height: 1.4em;} */
}

.logo_margin {
	margin: 0px 50px 0px 0px;
	padding-left: 0px;
}

.aboutMain {
	width: 100%;
	float: left;
	clear: both;
}


}/* END 768query */


/* Phone **********************/


@media (max-width: 30em) {
  
.sidebar { 
	clear: both;
	width: 100%;
	margin-top: 2em;
}


header ul li { 
	a {font-size: .8em;}
}

p {
	font-size: 1.1em;
}

}



















